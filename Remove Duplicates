class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        officer=0
        res=1
        cm=1
        j=len(nums)

        while(cm<j): 
            if nums[cm]==nums[cm-1]:
                cm=cm+1
                continue
            
            nums[officer+1]=nums[cm]
            officer+=1
            res+=1
            cm+=1
        return res
